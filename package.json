{
  "name": "AMGMarket",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "cross-env-shell jest",
    "ios": "npx react-native run-ios --simulator='iPhone 11'",
    "android": "npx react-native run-android --variant=Debug",
    "build:android:debug": "./gradlew assembleDebug",
    "build:android:release": "./gradlew assembleRelease -x bundleReleaseJsAndAssets",
    "lint": "cross-env-shell tslint --project ./tsconfig.json",
    "lint:fix": "cross-env-shell tslint --fix --project ./tsconfig.json",
    "pod-install": "cd ios && pod install && cd ..",
    "pod-update": "cd ios && pod update && cd ..",
    "pod-install-repo-update": "cd ios && pod install --repo-update && cd ..",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "android:release": "cd android && ./gradlew assembleRelease && cd .."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --ext .tsx,.ts,.js,.jsx"
    ]
  },
  "dependencies": {
    "@monterosa/react-native-parallax-scroll": "1.8.0",
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/datetimepicker": "3.0.4",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/picker": "1.8.1",
    "@react-navigation/bottom-tabs": "5.10.7",
    "@react-navigation/native": "5.7.6",
    "@react-navigation/stack": "5.9.3",
    "@reduxjs/toolkit": "1.4.0",
    "@types/react-native-loading-spinner-overlay": "0.5.2",
    "axios": "0.20.0",
    "formik": "2.2.1",
    "generate-unique-id": "2.0.1",
    "lodash": "^4.17.20",
    "moment": "2.29.1",
    "polished": "4.0.2",
    "react": "16.13.1",
    "react-addons-shallow-compare": "15.6.2",
    "react-addons-update": "15.6.2",
    "react-native": "0.63.3",
    "react-native-bootsplash": "3.1.1",
    "react-native-collapsible": "1.5.3",
    "react-native-device-info": "7.0.2",
    "react-native-gesture-handler": "1.8.0",
    "react-native-google-places-autocomplete": "2.1.1",
    "react-native-image-crop-picker": "0.35.1",
    "react-native-iphone-x-helper": "1.2.1",
    "react-native-keyboard-aware-scroll-view": "0.9.3",
    "react-native-loading-spinner-overlay": "2.0.0",
    "react-native-modal": "^11.5.6",
    "react-native-modal-datetime-picker": "9.0.0",
    "react-native-modal-select-list": "^1.1.0",
    "react-native-momosdk": "1.3.4",
    "react-native-paper": "4.2.0",
    "react-native-picker-select": "8.0.2",
    "react-native-reanimated": "1.13.1",
    "react-native-safe-area-context": "3.1.8",
    "react-native-screens": "2.11.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-vector-icons": "7.1.0",
    "react-native-webview": "^11.0.3",
    "react-number-format": "4.4.1",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-thunk": "2.3.0",
    "remote-redux-devtools": "0.5.16",
    "yup": "0.29.3"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.8.4",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/preset-env": "7.3.1",
    "@babel/runtime": "7.8.4",
    "@react-native-community/eslint-config": "1.1.0",
    "@types/jest": "24.0.23",
    "@types/lodash": "^4.14.165",
    "@types/react": "16.7.21",
    "@types/react-addons-shallow-compare": "^0.14.22",
    "@types/react-addons-update": "0.14.20",
    "@types/react-native": "0.57.32",
    "@types/react-native-snap-carousel": "3.8.2",
    "@types/react-native-vector-icons": "6.4.6",
    "@types/react-redux": "7.1.9",
    "@types/react-test-renderer": "16.0.3",
    "@types/redux-logger": "3.0.8",
    "@types/yup": "0.29.9",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "25.1.0",
    "babel-preset-airbnb": "3.2.0",
    "babel-preset-react-native": "4.0.1",
    "cross-env": "5.2.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-import-resolver-react-native": "0.2.0",
    "eslint-import-resolver-typescript": "2.2.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "22.1.2",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.12.2",
    "eslint-plugin-react-native": "3.7.0",
    "eslint-watch": "4.0.2",
    "husky": "4.3.0",
    "jest": "25.1.0",
    "lint-staged": "10.4.0",
    "metro-react-native-babel-preset": "0.59.0",
    "react-addons-test-utils": "15.6.2",
    "react-native-typescript-transformer": "1.2.11",
    "react-test-renderer": "16.13.1",
    "redux-logger": "3.0.6",
    "regenerator-runtime": "0.13.1",
    "ts-jest": "24.0.2",
    "tslint": "5.16.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-react": "5.0.0",
    "typescript": "3.7.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!ios/**",
      "!android/**"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "engines": {
    "node": ">=10",
    "npm": ">=6.0.0",
    "yarn": ">=1.9.2"
  }
}
